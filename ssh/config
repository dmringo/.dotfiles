Host moons trucks b146-*
  HostName %h.cs.unm.edu
  User davidringo
  IdentityFile ~/.ssh/id_rsa

Host darwin
  HostName darwin-fe.lanl.gov
  User dringo
  IdentityFile ~/.ssh/id_rsa

# Setup for sharing connections, see:
# https://puppet.com/blog/speed-up-ssh-by-reusing-connections
Host *
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h-%p
  ControlPersist 600